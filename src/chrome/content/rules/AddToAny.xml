<ruleset name="AddToAny">

	<target host="*.addtoany.com" />


	<!--	Set by static:
				-->
	<securecookie host="^\.addtoany\.com$" name="^uvc$" />
	<securecookie host="^\.static\.addtoany\.com$" name=".+" />


	<rule from="^http://static\.addtoany\.com/"
		to="https://static.addtoany.com/" />

</ruleset>